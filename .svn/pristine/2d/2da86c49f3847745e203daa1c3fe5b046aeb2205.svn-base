.banner{
    width: 100%;
    height: 540px;
    background: url("../imgs/duoyuanshengxue/banner.png") center center;
}
.part{
    width: 1200px;
    margin: 0 auto;
    color:#333;
    letter-spacing: 1px;
    overflow: hidden;
}
.part1{
    background-color: #fff;
    font-weight: bold;
}
.part1 .part{
    padding: 135px 0 50px;
    background-image: url("../imgs/duoyuanshengxue/bg.png") ;
    background-position: 0 100px;
    background-repeat:no-repeat ;
}
.part2{
    background: #F5F7F9;
}
.pTitle{
    font-size: 16px;
    margin-bottom: 12px;
}
.pContent{
    font-size:13px;
    line-height: 26px;
    margin-bottom: 36px;
}
.pubTitle{
    font-size: 38px;
    font-weight: bold;
    text-align: center;
}
.jianTitle{
    overflow: hidden;
}
.jianTitle1{
    padding: 0 25px;
    line-height: 58px;
    background:#0079FE;
    font-size: 26px;
    color:#fff;
    font-weight: bolder;
    float: left;
}
.jianTitle2{
    float: left;
    width: 0;
    height: 0;
    border-top: 29px solid;
    border-bottom:29px solid;
    border-left:25px solid;
    border-color:  transparent  #0079FE transparent ;
}
.part2 .list{
    display: flex;
    justify-content: space-between;
    height: 360px;
    margin: 50px auto;
}
.part2 .list li{
    width: 325px;
    height: 100%;
    text-align: center;
    padding-top: 30px;
    border-radius: 10px;
}
.part2 .list li p{
    color:#fff;
    font-weight: bolder;
}
.part2 .list li p span{
    display: inline-block;
    width: 60px;
    height: 8px;
    border-radius: 4px;
    background: #fff;
}
.part3{
    background: #fff;
}
.part3 .content{
    height: 840px;
    background: url("../imgs/duoyuanshengxue/zonghepingjia.png") center 0  no-repeat;
}
.part4{
    background: #F5F7F9;
    padding-bottom: 40px;
}
.part4 .list{
    height: 310px;
    display: flex;
    justify-content: space-between;
}
.part4 .list li{
    width: 158px;
    font-weight: bolder;
    text-align: center;
}
.part4 .list li img {
    width: 159px;
    height: 159px;
    display: block;
    margin: 0 auto;
}
.part4 .list li .p1{
    padding: 30px 0 20px;
    font-size: 23px;

}
.p2{
    font-size: 13px;
    color:#1A1A1A;
    line-height: 24px;
}
.part5{
    background: #fff;
    padding-bottom: 40px;
}
.part5 img{
    display: block;
    margin: 0 auto;
}
.part6{
    background: #F5F7F9;
}
.part6 .list{
    height: 325px;
}
.part6 .list li{
    width: 326px;
    height: 358px;
    padding-top: 168px;
}
.part6 .list li:nth-of-type(1){
    background: url('../imgs/duoyuanshengxue/fangxiang1.png');
}
.part6 .list li:nth-of-type(2){
    background: url('../imgs/duoyuanshengxue/fangxiang2.png');
}
.part6 .list li:nth-of-type(3){
    background: url('../imgs/duoyuanshengxue/fangxiang3.png');
}
.part9{
    background: #fff;
}
.part9 .list li image{
    display: block;
    width: 100%;
}
.part9 .list li .p1{
    color:#1A1A1A;
    font-size: 20px;
    margin: 20px auto 10px;
}
.part9 .list li .p2{
    color:#1A1A1A;
    font-size: 13px;
}
.part12 .list {
    height: 360px;
}
table{
    width: 100%;
    margin-bottom: 50px;
}
table,th,td{
    border:1px solid #97AFDC;
    border-collapse: collapse;
    font-size: 20px;
    line-height: 70px;
    color:#333333;
    text-align: center;
}
th{
   background:#CFD5EA;

}
tr:nth-of-type(2) th{
    background:#E9EBF5;
}
td{
    background:#F5F6FA;
}