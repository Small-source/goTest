<script>
    var before = $('body,html').scrollTop()
    $(window).on('scroll',function () {
        var after = $('body,html').scrollTop()
        if(after<76){
            $('header').show()
            return ;
        }
        if(after>before){
            $('header').hide()
        }else{
            $('header').show()
        }
        before = after;
    })
</script>
